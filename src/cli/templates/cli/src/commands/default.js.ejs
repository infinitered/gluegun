<% if (props.language === "typescript") { %>
import { GluegunCommand } from 'gluegun'
<% } %>

const command<%= (props.language === "typescript") ? ": GluegunCommand" : "" %> = {
  name: '<%= props.name %>',
  run: async toolbox => {
    const { print } = toolbox

    print.info('Welcome to your CLI')
  },
}

module.exports = command
