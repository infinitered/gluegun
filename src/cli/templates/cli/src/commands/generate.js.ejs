module.exports = {
  name: 'generate',
  alias: ['g'],
  run: async (context) => {
    const { parameters, template: { generate } } = context

    const name = parameters.first

    await generate({
      template: 'model.<%= props.extension %>.ejs',
      target: `models/${name}-model.<%= props.extension %>`,
      props: { name }
    })
  }
}
